{
	"luci-app-wolh": {
		"description": "Grant access to wake-on-lan executables",
		"read": {
			"ubus": {
				"luci-rpc": [ "getHostHints" ]
			},
			"uci": [ "etherwake", "dhcp", "wolh" ]
		},
		"write": {
			"uci": [ "wolh" ],
			"file": {
				"/usr/bin/etherwake": [ "exec" ],
				"/usr/bin/wol": [ "exec" ]
			}
		}
	}
}
